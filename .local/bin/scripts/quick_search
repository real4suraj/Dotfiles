#!/bin/bash
url=""
search="$(echo -e "\ngithub(real4suraj)\ngithub(real4suraj2)\nstack overflow\narch\nyoutube\nwhatsapp\n" | dmenu -nf "$(xrdb -q | sed -n /*.color14/p | cut -f2)" -nb "$(xrdb -q | sed -n /*.color0/p | cut -f2)" -sb "$(xrdb -q | sed -n /*.color6/p | cut -f2)" -sf "$(xrdb -q | sed -n /*.color0/p | cut -f2)" -p "Web")"
case "$search" in
  "") url="https://duckduckgo.com" ;;
  "github(real4suraj)") url="https://github.com/real4suraj" ;;
  "github(real4suraj2)") url="https://github.com/real4suraj2" ;;
  "stack overflow") url="https://stackoverflow.com" ;;
  "youtube") url="https://youtube.com" ;;
  "arch") url="https://archlinux.org" ;;
  "whatsapp") url="https://web.whatsapp.com" ;;
  *) url="https://google.com/search?q=$search" ;;
esac
brave --new-window "$url" > /dev/null 2>&1
xdotool key "Alt+m"
